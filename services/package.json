{
  "name": "distritv",
  "version": "1.0.0",
  "description": "Content distribution system",
  "main": "index.js",
  "scripts": {
    "start": "pnpm --stream -r start",
    "start:dev": "pnpm --parallel --stream -r run start:dev",
    "start:api:dev": "pnpm  --filter \"./api\" --filter \"./packages/**\" --parallel --stream -r run start:dev",
    "start:dashboard:dev": "pnpm  --filter \"./dashboard\" --filter \"./packages/**\" --parallel --stream -r run start:dev",
    "build": "pnpm --stream -r run build",
    "clean": "rm -rf .parcel-cache && pnpm -r run clean"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "dashboard": "^0.0.1",
    "tslib": "^2.6.2"
  }
}
